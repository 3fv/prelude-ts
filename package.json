{
    "name": "prelude.ts",
    "version": "0.3.0",
    "description": "A typescript functional programming library",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/emmanueltouzery/prelude.ts.git"
    },
    "dependencies": {
        "hamt_plus": "1.0.2"
    },
    "devDependencies": {
        "@types/mocha": "2.2.41",
        "mocha": "3.4.2",
        "typedoc": "0.7.2",
        "benchmark": "2.1.4",
        "typescript": "next",
        "browserify": "14.4.0"
    },
    "keywords": [
        "typescript",
        "functional-programming",
        "immutable",
        "collections"
    ],
    "author": "Emmanuel Touzery",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/emmanueltouzery/prelude.ts/issues"
    },
    "homepage": "https://github.com/emmanueltouzery/prelude.ts#readme",
    "scripts": {
        "prepublish": "tsc && node ./node_modules/browserify/bin/cmd.js -s prelude_ts dist/src/index.js -o dist/src/prelude_ts.js",
        "test": "tsc && ./node_modules/mocha/bin/mocha --timeout 10000 ./dist/tests/*.js",
        "clean": "rm -Rf ./dist",
        "docgen": "./node_modules/typedoc/bin/typedoc --mode file --out apidoc --excludePrivate --excludeExternals --excludeNotExported --ignoreCompilerErrors src/index.ts",
        "benchmarks": "tsc && node ./dist/benchmarks/bench.js"
    }
}
